include layout.jade
.header-top
	.container
		img(src="assets/image/banner_top.jpg")
		.row
			.col-md-7.top
				ul.list-unsyte.list-inline 
					each item__menu in["LIBERO.PE", "ELPOPULAR.PE", "FUNDACIÓN GUSTAVO MOHME LLONA"]
						li: a.link-menu(href="#")= item__menu
			.col-md-5.top
				button.boton SUSCRIBETE AL BOLETÍN
				i.i.fa.fa-facebook.facebook
				i.i.fa.fa-twitter.twitter
				i.i.fa.fa-google-plus.google
				i.i.fa.fa-envelope.msg
				i.i.fa.fa-rss.w
				button.buscar BUSCAR
					i.fa.fa-search
.header
	.container
		.row.republica
			.col-md-4.logo
				img(src="assets/image/logo-la-republica.jpg")
			.col-md-4.text-center
				.titulo
					p 
						strong DOMINGO
				.lider
					p 
						strong HABLA LA LIDEREZA DEL NACIONALISMO
			.col-md-4
				.caja-autor.text-center
					img.foto(src="assets/image/img_author.jpg")
					.autor
						p 
							strong La corrupsión, los origenes
						p por: Gustavo Mohme Llona
		.row.menu
			nav.navbar.navbar-custom
				.navbar-header
					button.navbar-toggle.collapsed(type="button",class="navbar-toggle",data-toggle="collapse",data-target="#navbars")
						i.fa.fa-bars
				.collapse.navbar-collapse#navbars
					ul.nav.navbar-nav
						li.hidden
							a(href="#")
						li
							a(href="#") POLÍTICA
						li
							a(href="#") ECONOMÍA
						li
							a(href="#") SOCIEDAD
						li
							a(href="#") ELECC. REGIONALES
						li
							a(href="#") DEPORTES
						li
							a(href="#") CHAMPIONS L.
						li
							a(href="#") ESPECTÁCULOS
						li
							a(href="#") REGIONALES
						li
							a(href="#") MÁS SECCIONES
		.row.temas
			.col-md-12
				ul.list-unsyte.list-inline.list-temas
					each item__menu in["TEMAS DEL DÍA", "<", "Globos de oro", "Youtube", "PNP", "Charlie Hedbod","FC Barceñlona", "Alianza Lima", "Balón de Oro","Alianza Lima", "Balón", ">"]
						li: a.link-menu(href="#")= item__menu
